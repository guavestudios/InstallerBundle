{
	"name": "guave/installer-bundle",
	"description": "Installer Scripts for Contao 4.9+",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Christopher Stadler",
			"email": "christopher.stadler@guavestudios.com",
			"homepage": "https://github.com/guavestudios"
		}
	],
	"require": {
		"php": "^7.4 || ^8.0",
		"contao/core-bundle": "^4.9"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"autoload": {
		"psr-4": {
			"Guave\\InstallerBundle\\": "src/"
		}
	},
	"bin": [
		"bin/project-dirs",
		"bin/project-files"
	],
	"minimum-stability": "stable",
	"support": {
		"issues": "https://github.com/guavestudios/InstallerBundle/issues",
		"source": "https://github.com/guavestudios/InstallerBundle"
	}
}
